\n  }\r\n\r\n  .cityEvent::before {\r\n    width: 2.5rem;\r\n    height: 2.5rem;\r\n    position: absolute;\r\n    left: 0;\r\n    top: 0;\r\n    border-top: 0.25rem solid rgb(130, 18, 171);\r\n    border-left: 0.25rem solid rgb(130, 18, 171);\r\n    content: '';\r\n    display: block;\r\n  }\r\n  .cityEvent::after {\r\n    width: 2.5rem;\r\n    height: 2.5rem;\r\n    position: absolute;\r\n    right: 0;\r\n    top: 0;\r\n    border-top: 0.25rem solid rgb(130, 18, 171);\r\n    border-right: 0.25rem solid rgb(130, 18, 171);\r\n    content: '';\r\n    display: block;\r\n  }\r\n  .footerBoder {\r\n    position: absolute;\r\n    top: 0;\r\n    left: 0;\r\n    right: 0;\r\n    bottom: 0;\r\n  }\r\n  .footerBoder::before {\r\n    width: 2.5rem;\r\n    height: 2.5rem;\r\n    position: absolute;\r\n    left: 0;\r\n    bottom: 0;\r\n    border-bottom: 0.25rem solid rgb(130, 18, 171);\r\n    border-left: 0.25rem solid rgb(130, 18, 171);\r\n    content: '';\r\n    display: block;\r\n  }\r\n  .footerBoder::after {\r\n    width: 2.5rem;\r\n    height: 2.5rem;\r\n    position: absolute;\r\n    right: 0;\r\n    bottom: 0;\r\n    border-bottom: 0.25rem solid rgb(130, 18, 171);\r\n    border-right: 0.25rem solid rgb(130, 18, 171);\r\n    content: '';\r\n    display: block;\r\n  }\r\n  .svg {\r\n    width: 2.8125rem;\r\n    height: 2.8125rem;\r\n    margin-left: .3125rem;\r\n  }\r\n  .right {\r\n    // display: flex;\r\n    // flex-direction: column;\r\n    width: 18.75rem;\r\n    // background-image: url(@/assets/bg/line_img.png);\r\n    // background-size: contain;\r\n    // background-position: left center;\r\n    // background-repeat: no-repeat;\r\n    padding: 1.25rem 1.25rem;\r\n    overflow: scroll;\r\n    &::-webkit-scrollbar {\r\n      width: 0 !important;\r\n    }\r\n    // .cityEvent{\r\n    //   flex: 0;\r\n    // }\r\n    // h3{\r\n    //   flex: 0;\r\n    // }\r\n    ul > li {\r\n      list-style: none;\r\n      padding: 1.35rem;\r\n      pointer-events: auto;\r\n      